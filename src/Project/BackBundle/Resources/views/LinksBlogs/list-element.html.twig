<tr id="fila_{{element.id}}">
	<td >{{element.id}}</td>
    <td >{{element.name}}</td>
    <td class="hidden-phone" >{%if element.url != ''%}<a href="{{element.url}}" target="_blank">{{element.url[:50] ~ '...'}}</a>{%endif%}</td>
    <td class="hidden-phone" >{%if element.urles != ''%}<a href="{{element.urles}}" target="_blank">{{element.urles[:50] ~ '...'}}</a>{%endif%}</td>
    <td >{{element.website}}</td>
    
        {% include 'ProjectBackBundle:Helpers:listado-edit-delete-basico.html.twig'  with {'tipo':nombreClase,'rutaEditar':'project_back_'~nombreRuta~'_edit'}  %}
    
</tr>