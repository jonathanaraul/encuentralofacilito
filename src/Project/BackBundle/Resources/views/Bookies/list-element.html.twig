<tr id="fila_{{element.id}}">
	<td >{{element.id}}</td>
	<td >{%if element.logo != ''%}<img src="{{asset(element.getWebPath)}}">{%endif%}</td>
    <td >{{element.nombre}}</td>
    <td class="hidden-phone" >{%if element.url != ''%}<a href="{{element.url}}" target="_blank">{{element.url[:50] ~ '...'}}</a>{%endif%}</td>
    <td class="hidden-phone">
       {%if element.patrocinador%}
       <span class="label label-info arrowed arrowed-righ">Si</span>
       {%else%}
       <span class="label label-inverse arrowed-in">No</span>
       {%endif%}
    </td>
    <td class="hidden-phone">
       {%if element.licenciaEsp%}
       <span class="label label-info arrowed arrowed-righ">Si</span>
       {%else%}
       <span class="label label-inverse arrowed-in">No</span>
       {%endif%}
    </td>
    
    {% include 'ProjectBackBundle:Helpers:listado-edit-delete-basico.html.twig'  with {'tipo':nombreClase,'rutaEditar':'project_back_'~nombreRuta~'_edit'}  %}
    
</tr>