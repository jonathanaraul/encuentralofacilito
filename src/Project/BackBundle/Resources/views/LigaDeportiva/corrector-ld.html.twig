{% extends '::base.html.twig' %}



{% block pagesScriptsBottom %}
{%endblock%}

{% block pageContent  %}
<div class="main-container container-fluid" id="sinlogin">
	<a class="menu-toggler" id="menu-toggler" href="#"> <span class="menu-text"></span> </a>

	{% include '::sidebar.html.twig'  with {'clase': 'gris'}  %}

	<div class="main-content">

		{# include 'ProjectBackBundle:Architecture:breadcrumb.html.twig'  with {'clase': 'gris'} #}
		<div class="page-content">
			{% include 'ProjectBackBundle:'~nombreClase~':page-header.html.twig' with {'subtitulo':'Corrector LD'} %}
			
			<div class="row-fluid">
				<div class="span12">
					<!--PAGE CONTENT BEGINS-->
					<!--PAGE CONTENT BEGINS-->
					<div class="row-fluid">
						<div class="span12">
							<div class="widget-box ">
								<div class="widget-header header-color-blue">
									<h4 id="titulo-corrector">Corrector LD {{nombreMes}} - {{anio}}</h4>
									<input type="hidden" id="codigo-fecha-corrector" value="{{mes}}-{{anio}}">
									<span class="widget-toolbar">
										<a href="#" data-action="collapse">
											<i class="icon-chevron-up"></i>
										</a>
									</span>
								</div>
								<div class="widget-body" style="background-color: #eff3f8;">
									<div class="widget-main">
										<div class="row-fluid">
											<div id="tracking_site_filtro" class="form-horizontal">
												<div>
													<div class="controls">
														<button type="submit" id="corrector-ld-mes-anterior" name="tracking_site_filtro[save]" class="btn btn-info">Mes Anterior</button>
													</div>
												</div>
											</div>
									    </div>
								    </div>
							    </div>
						    </div><!--/span-->
					    </div>	
					</div>
					<div class="row-fluid" id="correctorLD-loader" style="display:none">
						<div class="span12">
							<p style="text-align: center;margin-top: 100px;">
								<img src="{{asset('admin/images/loader.gif')}}">
							</p>
						</div>
					</div>
					<div class="row-fluid" id="tabla-elementos" >
						<div class="span12">
							<div id="sample-table-2_wrapper" class="dataTables_wrapper" role="grid">
								<table id="sample-table-2" class="table table-striped table-bordered table-hover">
									<thead id="cabecera-elementos">
                                       {% include 'ProjectBackBundle:'~nombreClase~':cabecera-corrector-ld.html.twig' %}
									</thead>
									<tbody id="contenido-elementos">
										{% include 'ProjectBackBundle:'~nombreClase~':list-element-corrector-ld.html.twig' %}
									</tbody>
								</table>
								<div class="row-fluid" style="margin-top: -20px;">
									<div class="span6">

									</div>
									<div class="span6">
									</div>
								</div>
							</div>
						</div><!--/span-->
					</div><!--/row-->
					<div class="hr hr-18 dotted hr-double" id="antes-mensaje-error"></div>
					<!--PAGE CONTENT ENDS-->
				</div><!--/.span-->
			</div><!--/.row-fluid-->
		</div><!--/.page-content-->
	</div><!--/.main-content-->
</div><!--/.main-container-->

{%endblock%}